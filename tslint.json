{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "build-assets/**"
    ]
  },
  "rules": {
    "no-console": false,
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "max-line-length": {
      "options": [
        200
      ]
    },
    "member-access": false,
    "member-ordering": [
      false,
      {
        "order": [
          "public-",
          "private-",
          "protected-"
        ],
        "alphabetize": true
      }
    ],
    "no-consecutive-blank-lines": false,
    "no-var-requires": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "prefer-for-of": false,
    "variable-name": false
  }
}